{
  "name": "spotify-library-tone",
  "version": "0.0.1",
  "description": "Analyze the tone of your Spotify library with IBM Watson",
  "main": "index.js",
  "scripts": {
    "start": "better-npm-run start",
    "build": "better-npm-run build",
    "dev": "better-npm-run dev",
    "lint": "eslint --ignore-path .gitignore .",
    "start:dev": "export NODE_ENV=development && nodemon server/app.js"
  },
  "betterScripts": {
    "build": {
      "command": "webpack --config webpack/prod.config.js --progress --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "node server/app.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "dev": {
      "command": "node server/app.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "dependencies": {
    "better-npm-run": "0.0.6",
    "bluebird": "^3.2.1",
    "body-parser": "^1.14.2",
    "classnames": "^2.2.5",
    "clone": "^1.0.2",
    "cookie-parser": "^1.4.1",
    "dotenv": "^5.0.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^1.0.2",
    "eslint-plugin-react": "^3.16.1",
    "express": "^4.13.4",
    "fs": "0.0.1-security",
    "history": "^1.17.0",
    "lodash": "^4.17.5",
    "lowdb": "^1.0.0",
    "morgan": "^1.6.1",
    "object-assign": "^4.0.1",
    "observe": "^1.4.1",
    "onoff": "^1.2.0",
    "querystring": "^0.2.0",
    "react": "^0.14.9",
    "react-dom": "^0.14.9",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.2.1",
    "react-router": "^2.0.0-rc5",
    "react-router-redux": "^3.0.0",
    "react-transform-hmr": "^1.0.2",
    "redbox-react": "^1.2.2",
    "redux": "^3.2.1",
    "redux-thunk": "^1.0.3",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "spotify-web-api-js": "^0.23.0",
    "spotify-web-api-node": "^2.2.0",
    "style-loader": "^0.13.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.0",
    "autoprefixer-loader": "^3.2.0",
    "babel": "^6.3.26",
    "babel-core": "^6.4.5",
